{
  "presets": [["@babel/preset-env",
    {
      "targets": {
        "esmodules": true
      }
    }], "@babel/preset-typescript",["@babel/preset-react", {
    "runtime": "automatic"
  }]],
  "plugins": [["@babel/plugin-transform-runtime",
    {
      "regenerator": true
    }
  ]],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}